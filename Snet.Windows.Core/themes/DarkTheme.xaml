<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sv="clr-namespace:Snet.Windows.Core"
    xmlns:ui="http://materialdesigninxaml.net/winfx/xaml/themes">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Dark.xaml" />
        <ResourceDictionary Source="pack://application:,,,/MaterialDesignColors;component/Themes/Recommended/Primary/MaterialDesignColor.Grey.xaml" />
        <ResourceDictionary Source="pack://application:,,,/MaterialDesignColors;component/Themes/Recommended/Secondary/MaterialDesignColor.Cyan.xaml" />
        <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesign3.Defaults.xaml" />

        <ResourceDictionary Source="pack://application:,,,/Snet.Windows.Core;component/resources/Icons.xaml" />
        <ResourceDictionary Source="pack://application:,,,/Snet.Windows.Core;component/resources/WindowIcon.xaml" />
        <ResourceDictionary Source="pack://application:,,,/Snet.Windows.Core;component/themes/style/Dark.xaml" />
        <ResourceDictionary Source="pack://application:,,,/Snet.Windows.Core;component/themes/WindowBase.xaml" />
    </ResourceDictionary.MergedDictionaries>
    <Style
        x:Key="{x:Type sv:WindowBase}"
        BasedOn="{StaticResource WindowBaseStyle}"
        TargetType="{x:Type sv:WindowBase}" />
</ResourceDictionary>